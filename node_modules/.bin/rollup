#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/1.31.0/node_modules/rollup/dist/bin/rollup/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/1.31.0/node_modules/rollup/dist/bin/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/1.31.0/node_modules/rollup/dist/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/1.31.0/node_modules/rollup/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/1.31.0/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/rollup/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules:/Users/hronro/temp/node_modules:/Users/hronro/node_modules:/Users/node_modules:/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/rollup/dist/bin/rollup/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/rollup/dist/bin/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/rollup/dist/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
