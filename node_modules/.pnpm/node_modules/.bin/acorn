#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.0/node_modules/acorn/bin/acorn/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.0/node_modules/acorn/bin/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.0/node_modules/acorn/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.0/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules:/Users/hronro/temp/node_modules:/Users/hronro/node_modules:/Users/node_modules:/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/node_modules/acorn/bin/acorn/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/Users/hronro/temp/crypto-browserify-esm/node_modules/.pnpm/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
